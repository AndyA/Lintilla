<!DOCTYPE html>
<html>
  <head>
    <title>Elvis Images</title>
    <script src="/js/jquery-1.9.1.min.js"></script>
    <script src="/js/jquery-ui-1.10.1.custom.min.js"></script>
    <script src="/js/urlparser.js"></script>
    <script src="/js/scaler.js"></script>
    <script src="/js/magicplayer.js"></script>
    <script src="/js/colourwheel.js"></script>
    <script src="/js/join.js"></script>
    <script src="/js/elvis.js"></script>
    <link rel="shortcut icon" href="/favicon.ico" type="image/x-icon">
    <link rel="icon" href="/favicon.ico" type="image/x-icon">
    <link rel="stylesheet" href="/css/style.css" />
    <link rel="stylesheet"
      href="//maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css">
    <script>
      ds = "<%= ds | html %>";
    </script>
  </head>
  <body>
    <div id="container">
      <div id="popup">
        <span class="ident"><a href="/">ELVIS Image Search</a></span>
        <form method="GET" action="/search">
          <input type="text" class="query" name="q" value="<%= q | html %>" size="40" />
          <button type="submit" class="icon-search fa fa-search fa-lg"></button>
        </form>
      </div>
      <% content %>
    </div>
  </body>
</html>
